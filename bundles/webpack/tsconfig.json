{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"declaration": true,
		"importHelpers": true,
		"inlineSources": false,
		"declarationMap": false,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictBindCallApply": true,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"moduleResolution": "Node",
		"module": "ES2020",
		"target": "ES2021",
		"lib": [
			"ES2021",
			"dom"
		],
		"strict": true,
		"strictPropertyInitialization": false,
		"removeComments": true,
		"jsx": "react",
		"allowJs": true
	}
}